{
    "name": "xx",
    "type": "project",
    "description": "xxxxx",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-simplexml": "0.1",
        "awobaz/compoships": "^1.1",
        "doctrine/dbal": "^2.10",
        "facade/ignition": "^1.4",
        "fideloper/proxy": "^4.0",
        "knplabs/github-api": "^2.12",
        "laravel/framework": "^6.3",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "m4tthumphrey/php-gitlab-api": "^9.18",
        "nunomaduro/collision": "^3.0",
        "pda/pheanstalk": "^4.0",
        "php-http/guzzle6-adapter": "^2.0",
        "phpseclib/phpseclib": "^2.0",
        "predis/predis": "^1.1",
        "staudenmeir/laravel-upsert": "^1.2",
        "symfony/yaml": "^5.0",
        "vinkla/hashids": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "sun-asterisk/coding-standard": "^0.2.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": "phpcs --basepath=.",
        "autofix": "phpcbf",
        "test": "phpunit",
        "coverage": "phpunit --coverage-html=coverage --coverage-clover ./coverage.xml",
        "check": [
            "@sniff",
            "@test"
        ]
    }
}
